{
  "luci-app-cpu-perf": {
    "source": {
      "url": "https://github.com/gSpotx2f/luci-app-cpu-perf.git"
    },
    "build": {
      "dependencies": {
        "opkg": [
          "git",
          "make",
          "gcc",
          "pkg-config",
          "libtool",
          "automake",
          "autoconf",
          "gettext",
          "lua",
          "wget",
          "tar",
          "unzip"
        ],
        "apk": [
          "git",
          "build-base",
          "gcc",
          "pkgconf",
          "libtool",
          "automake",
          "autoconf",
          "gettext",
          "lua",
          "wget",
          "tar",
          "unzip"
        ]
      },
      "commands": {
        "default": {
          "x86_64": "cd /tmp/aios/build/luci-app-cpu-perf && ./scripts/feeds update -a && ./scripts/feeds install -a && make package/luci-app-cpu-perf/compile",
          "armv7l": "cd /tmp/aios/build/luci-app-cpu-perf && ./scripts/feeds update -a && ./scripts/feeds install -a && make package/luci-app-cpu-perf/compile",
          "mipsel": "cd /tmp/aios/build/luci-app-cpu-perf && ./scripts/feeds update -a && ./scripts/feeds install -a && make package/luci-app-cpu-perf/compile",
          "mips": "cd /tmp/aios/build/luci-app-cpu-perf && ./scripts/feeds update -a && ./scripts/feeds install -a && make package/luci-app-cpu-perf/compile"
        }
      }
    }
  }
}
