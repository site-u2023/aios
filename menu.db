# AIOSメニュー設定データベース

[openwrt-config]
magenta MENU_INTERNET selector internet-config
blue MENU_SYSTEM selector system-config
cyan MENU_PACKAGES download "package-config.sh" "chmod" "load"
green MENU_ADBLOCKER download "adblocker-dns.sh" "chmod" "load"
yellow MENU_ACCESSPOINT download "accesspoint-setup.sh" "chmod" "load"
red MENU_OTHERS download "other-utilities.sh" "chmod" "load"
white MENU_EXIT menu_exit
white_black MENU_REMOVE remove_exit

[internet-config]
magenta MENU_INTERNET download "internet-config.sh" "chmod" "load"
white MENU_EXIT menu_exit
white_black MENU_RETURN return_menu

[system-config]
blue MENU_SYSTEM download "system-config.sh" "chmod" "load" 
white MENU_EXIT menu_exit
white_black MENU_RETURN return_menu
