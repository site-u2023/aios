#!/bin/sh

SCRIPT_VERSION="2025.02.23-00-00"

[default]
build_dir = /tmp/aios/build
openwrt_repo = https://git.openwrt.org/openwrt/openwrt.git

[luci-app-example]
source_url = https://github.com/example/luci-app-example.git
build_dependencies = make gcc libtool autoconf automake
yn = yes  # デフォルトでインストール確認をする
hidden = no # デフォルトで非表示インストールしない

[build-commands]
23.05.4 = make package/luci-app-example/compile V=s
22.03.3 = make package/luci-app-example/compile -j$(nproc)
19.07 = make package/luci-app-example/compile
default = make package/luci-app-example/compile

