# AIOSメニュー設定データベース

[openwrt-config]
MENU_INTERNET selector internet-config
MENU_SYSTEM selector system-config
MENU_PACKAGES selector package-config
MENU_ADBLOCKER download "adblocker-dns.sh" "chmod" "load"
MENU_ACCESSPOINT download "accesspoint-setup.sh" "chmod" "load"
MENU_OTHERS download "other-utilities.sh" "chmod" "load"

[internet-config]
MENU_MAPE selector internet-mape
MENU_DSLITE selector internet-dslite
MENU_PPPOE selector internet-pppoe
MENU_DHCP selector internet-dhcp
MENU_V&MIG selector internet-v6mig

[system-config]
MENU_HOSTNAME selector system-hostname
MENU_PASSWORD selector system-password
MENU_WIFI selector system-wifi
MENU_TIMEZONE selector system-timezone
MENU_LED selector system-led
MENU_REBOOT selector system-reboot

[package-config]
MENU_AUTO_INSTALL selector package-auto
MENU_MANUAL_SELECT selector package-manual
MENU_UPDATE selector package-update
MENU_CUSTOM selector package-custom

# インターネット設定のサブセクション
[internet-mape]
MENU_MAPE_CONFIG download "map-e.sh" "chmod" "load"

[internet-dslite]
MENU_DSLITE_CONFIG download "internet-dslite.sh" "chmod" "load"

[internet-pppoe]
MENU_PPPOE_CONFIG download "internet-pppoe.sh" "chmod" "load"

[internet-dhcp]
MENU_DHCP_CONFIG download "internet-dhcp.sh" "chmod" "load"

[internet-v6mig]
MENU_V&MIG_CONFIG download "v6mig-prov.sh" "chmod" "load"

# システム設定のサブセクション
[system-hostname]
MENU_HOSTNAME_CONFIG download "system-hostname.sh" "chmod" "load"

[system-password]
MENU_PASSWORD_CONFIG download "system-password.sh" "chmod" "load"

[system-wifi]
MENU_WIFI_CONFIG download "system-wifi.sh" "chmod" "load"

[system-timezone]
MENU_TIMEZONE_CONFIG download "system-timezone.sh" "chmod" "load"

[system-led]
MENU_LED_CONFIG download "system-led.sh" "chmod" "load"

[system-reboot]
MENU_REBOOT_CONFIG download "system-reboot.sh" "chmod" "load"

# パッケージ設定のサブセクション
[package-auto]
MENU_AUTO_INSTALL_CONFIG download "package-auto.sh" "chmod" "load"

[package-manual]
MENU_MANUAL_SELECT_CONFIG download "package-manual.sh" "chmod" "load"

[package-update]
MENU_UPDATE_CONFIG download "package-update.sh" "chmod" "load"

[package-custom]
MENU_CUSTOM_CONFIG download "package-custom.sh" "chmod" "load"
