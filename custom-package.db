"luci-app-cpu-perf": {
  "source": {
    "url": "https://github.com/gSpotx2f/luci-app-cpu-perf.git"
  },
  "build": {
    "dependencies": {
      "opkg": [
        "git",
        "make",
        "gcc",
        "pkg-config",
        "libtool",
        "automake",
        "autoconf",
        "gettext",
        "lua",
        "wget",
        "tar",
        "unzip",
        "perl",
        "perlbase-findbin"
      ]
    },
    "commands": {
      "default": {
        "x86_64": "make package/luci-app-cpu-perf/compile",
        "armv7l": "make package/luci-app-cpu-perf/compile",
        "mipsel": "make package/luci-app-cpu-perf/compile",
        "mips": "make package/luci-app-cpu-perf/compile"
      }
    }
  }
}
