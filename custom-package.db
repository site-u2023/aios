{
  "default": {
    "dependencies": {
      "opkg": ["make", "gcc", "wget"]
    }
  },
  "luci-app-cpu-perf": {
    "source": {
      "url": "https://github.com/gSpotx2f/luci-app-cpu-perf.git"
    },
    "build": {
      "dependencies": {
        "opkg": [
          "git",
          "make",
          "gcc",
          "pkg-config",
          "libtool",
          "automake",
          "autoconf",
          "gettext",
          "lua",
          "wget",
          "tar",
          "unzip",
          "perl",
          "perlbase-findbin",
          "perlbase-file"
        ]
      },
      "commands": {
        "default": {
          "x86_64": "cd ../../ && ./scripts/feeds update -a && ./scripts/feeds install -a && cd package/luci-app-cpu-perf && make package/luci-app-cpu-perf/compile",
          "armv7l": "cd ../../ && ./scripts/feeds update -a && ./scripts/feeds install -a && cd package/luci-app-cpu-perf && make package/luci-app-cpu-perf/compile",
          "mipsel": "cd ../../ && ./scripts/feeds update -a && ./scripts/feeds install -a && cd package/luci-app-cpu-perf && make package/luci-app-cpu-perf/compile",
          "mips": "cd ../../ && ./scripts/feeds update -a && ./scripts/feeds install -a && cd package/luci-app-cpu-perf && make package/luci-app-cpu-perf/compile"
        }
      }
    }
  }
}
