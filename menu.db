# AIOSメニュー設定データベース

[MAIN_MENU_NAME]
MENU_INTERNET selector MENU_INTERNET
MENU_SYSTEM selector MENU_SYSTEM
MENU_PACKAGES selector MENU_PACKAGES
MENU_ADBLOCKER download "adblocker-dns.sh" "chmod" "load"
MENU_ACCESSPOINT download "accesspoint-setup.sh" "chmod" "load"
MENU_OTHERS download "other-utilities.sh" "chmod" "load"

[MENU_INTERNET]
MENU_INTERNET_AUTO selector MENU_INTERNET_AUTO
MENU_INTERNET_V&MIG selector MENU_INTERNET_V&MIG
MENU_INTERNET_MAPE selector MENU_INTERNET_MAPE
MENU_INTERNET_DSLITE selector MENU_INTERNET_DSLITE
MENU_INTERNET_PPPOE selector MENU_INTERNET_PPPOE

[MENU_INTERNET_AUTO]
MENU_INTERNET_AUTO download "internet-auto-config.sh" "chmod" "load"

[MENU_INTERNET_V&MIG]
MENU_INTERNET_V&MIG download "v6mig-prov.sh" "chmod" "load"

[MENU_INTERNET_MAPE]
MENU_INTERNET_MAPE_CONFIG download "map-e.sh" "chmod" "load"

[MENU_INTERNET_DSLITE]
MENU_INTERNET_DSLITE_CONFIG download "internet-dslite.sh" "chmod" "load"

[MENU_INTERNET_PPPOE]
MENU_INTERNET_PPPOE_CONFIG download "internet-pppoe.sh" "chmod" "load"

[MENU_SYSTEM]
MENU_HOSTNAME selector system-hostname
MENU_PASSWORD selector system-password
MENU_WIFI selector system-wifi
MENU_TIMEZONE selector system-timezone
MENU_LED selector system-led
MENU_REBOOT selector system-reboot

[MENU_PACKAGES]
MENU_PACKAGES_AUTO_INSTALL selector MENU_PACKAGES_AUTO_INSTALL
MENU_PACKAGES_MANUAL_INSTALL download "package-manual.sh" "chmod" "load"; package_manual_main
MENU_PACKAGES_SAMBA download "package-manual.sh" "chmod" "load"; package_samba
MENU_PACKAGES_LIST download "package-manual.sh" "chmod" "load"; package_list

[MENU_PACKAGES_AUTO_INSTALL]
MENU_PACKAGE_STANDARD download "package-auto.sh" "chmod" "load"; detect_and_run_installer standard
MENU_PACKAGE_MINIMAL download "package-auto.sh" "chmod" "load"; detect_and_run_installer minimal
MENU_PACKAGE_FULL download "package-auto.sh" "chmod" "load"; detect_and_run_installer full
