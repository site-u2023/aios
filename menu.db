# AIOSメニュー設定データベース

[MAIN_MENU_NAME]
MENU_INTERNET download "internet-auto-config.sh" "chmod" "load"
# MENU_INTERNET selector MENU_INTERNET
# MENU_SYSTEM selector MENU_SYSTEM
MENU_PACKAGES selector MENU_PACKAGES

[MENU_INTERNET]
# MENU_INTERNET_PPPOE selector MENU_INTERNET_PPPOE
MENU_INTERNET_MAPE selector MENU_INTERNET_MAPE
MENU_INTERNET_DSLITE selector MENU_INTERNET_DSLITE
MENU_INTERNET_MAPE_NURO selector MENU_INTERNET_MAPE_NURO

[MENU_INTERNET_PPPOE]
MENU_INTERNET_PPPOE menu_yn && download "internet-pppoe.sh" "chmod" "load" && internet_pppoe_main
MENU_INTERNET_RESTORE download "internet-pppoe.sh" "chmod" "load"; restore_pppoe

[MENU_INTERNET_MAPE]
MENU_INTERNET_MAPE_MAPOCNV6PLUS menu_yn && download "internet-map-e.sh" "chmod" "load" && internet_map_main
MENU_INTERNET_RESTORE download "internet-map-e.sh" "chmod" "load"; restore_mape
MENU_INTERNET_MAPE_TEST menu_yn && download "test-ocn-mape.sh "$API_SETUP" " "chmod" "load"

[MENU_INTERNET_DSLITE]
MENU_INTERNET_DSLITE_TRANS_EAST menu_yn && download "internet-ds-lite-config.sh" "chmod" "load" && internet_dslite_main "2403:7f00:4000:2000::126"
MENU_INTERNET_DSLITE_TRANS_WEST menu_yn && download "internet-ds-lite-config.sh" "chmod" "load" && internet_dslite_main "2403:7f00:c000:1000::126"
MENU_INTERNET_DSLITE_XPASS menu_yn && download "internet-ds-lite-config.sh" "chmod" "load" && internet_dslite_main "dgw.xpass.jp"
MENU_INTERNET_DSLITE_V6OPTION menu_yn && download "internet-ds-lite-config.sh" "chmod" "load" && internet_dslite_main "gw.example.v6option.ne.jp"
MENU_INTERNET_RESTORE download "internet-ds-lite-config.sh" "chmod" "load"; restore_dslite

[MENU_INTERNET_MAPE_NURO]
MENU_INTERNET_MAPE_NUROHIKARI menu_yn && download "internet-map-e-nuro.sh" "chmod" "load" && internet_map_nuro_main
MENU_INTERNET_RESTORE download "internet-map-e-nuro.sh" "chmod" "load"; restore_mape_nuro

[MENU_SYSTEM]
MENU_SYSTEM printf "\n%s\n" "$(color yellow "Coming soon")"

[MENU_PACKAGES]
MENU_PACKAGES_AUTO_INSTALL export PACKAGE_INSTALL_MODE="auto"; download "package.sh" "chmod" "load"; package_main
MENU_PACKAGES_MANUAL_INSTALL export PACKAGE_INSTALL_MODE="manual"; download "package.sh" "chmod" "load"; package_main
MENU_PACKAGES_LIST download "package.sh" "chmod" "load"; check_install_list
